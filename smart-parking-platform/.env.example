# Application
APP_NAME=SmartParkingPlatform
APP_ENV=development
DEBUG=true
SECRET_KEY=your-secret-key-change-in-production
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# PostgreSQL
POSTGRES_USER=parking_admin
POSTGRES_PASSWORD=parking_secret_123
POSTGRES_DB=smart_parking
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
DATABASE_URL=postgresql://parking_admin:parking_secret_123@localhost:5432/smart_parking

# Neo4j
NEO4J_URI=bolt://localhost:7687
NEO4J_USER=neo4j
NEO4J_PASSWORD=neo4j_parking_123

# Redis (for caching & pub/sub)
REDIS_URL=redis://localhost:6379/0

# Azure (optional â€” for cloud ETL)
AZURE_STORAGE_CONNECTION_STRING=
AZURE_SYNAPSE_CONNECTION_STRING=
AZURE_DATA_FACTORY_NAME=
AZURE_RESOURCE_GROUP=

# ML Models
MODEL_PATH=./ml/models
YOLO_MODEL=yolov8n.pt
CONFIDENCE_THRESHOLD=0.5
TESSERACT_CMD=/usr/bin/tesseract

# Camera Configuration
CAMERA_FEED_URL=rtsp://localhost:8554/parking
FRAME_SKIP=5
MAX_CAMERAS=10

# Frontend
VITE_API_URL=http://localhost:8000
VITE_WS_URL=ws://localhost:8000/ws
